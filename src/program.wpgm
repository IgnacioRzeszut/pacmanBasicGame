import example.*

program pacman {
	
	game.title("Pacman!")
	
	// l√≠mites del juego
	game.width(14)
	game.height(8)
	
	// fondo
	game.boardGround("pacmanBackground.jpg")
	
	// personaje principal
	game.addVisualCharacter(pacman)

	game.addVisual(new Rival(1))
	game.addVisual(new Rival(2))
		
	// que arranque el juego!
	game.start()
	
}